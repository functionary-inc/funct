{
  "private": false,
  "version": "4.0.5",
  "name": "@funct/core",
  "license": "MIT",
  "description": "The shared resources for all the packages for calling the functionary API.",
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "format": "prettier --write \"src/**/*.ts\" ",
