    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)?$": "../../babel.transformer.js"
    }
  },
  "dependencies": {
    "axios": "^1.2.0",
    "crypto": "^1.0.1",
    "lodash.throttle": "^4.1.1",
    "lodash.union": "^4.6.0"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.7",
    "@types/lodash.union": "^4.6.7"
  }
